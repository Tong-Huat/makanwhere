  <div class="card text-dark bg-light mb-3 est-card">
    <div class="card-header">
      <h1>Establishments Record</h1>
    </div>
    <div class="card-body">
      <table class="table table-striped table-sm table-responsive-md">
        <thead>
          <tr class="thead">
            <th scope="col">Index</th>
            <th scope="col">Name</th>
            <th scope="col">Area</th>
            <th scope="col">Actions</th>

          </tr>
        </thead>
        <tbody>
          <!-- Create an row for each sighting -->
          <% for (let i=0; i < dataObj.data.length; i +=1) {%>
            <tr>
              <th scope="row" class="theader">
                <%= i+1 %>
              </th>
              <td class="tdata">
                <a href="<%= `/listing/${dataObj.data[i].id}` %>">
                  <%= dataObj.data[i].name %>
                </a>
              </td>
              <td class="tdata">
                <%= dataObj.data[i].area %>
              </td>

              <td>
                <form action="/listing/<%= dataObj.data[i].id %>?_method=DELETE" method="POST">
                  <!-- <input type="submit" value="Delete" /> -->
                  <button><i class="fas fa-trash-alt"></i></button>
                </form>
              </td>

            </tr>
            <% }; %>
        </tbody>
      </table>
    </div>
  </div>
  <%- include('footer'); -%>